{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "extends": "./base.json",
    "compilerOptions": {
        "jsx": "react-jsx",
        "allowJs": true,
        "declaration": false,
        "declarationMap": false,
        "incremental": true,
        "lib": ["dom", "dom.iterable", "esnext"],
        "module": "esnext",
        "noEmit": true,
        "resolveJsonModule": true,
        "target": "es5"
    }
}
