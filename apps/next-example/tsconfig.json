{
    "extends": "@marshallku/typescript-config/react-library.json",
    "compilerOptions": {
        "target": "ES2020",
        "incremental": true,
        "paths": {
            "#/*": ["./*"]
        },
        "allowJs": true,
        "noEmit": true,
        "resolveJsonModule": true,
        "jsx": "preserve",
        "plugins": [
            {
                "name": "next"
            }
        ]
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
    "exclude": ["node_modules"]
}
